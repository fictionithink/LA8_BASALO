<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.graph.HelloController">
   <children>
      <AnchorPane fx:id="apPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" />
      <Button mnemonicParsing="false" onAction="#onSaveClicked" text="SAVE" textAlignment="CENTER">
         <VBox.margin>
            <Insets left="240.0" />
         </VBox.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#onAddCircleClicked" text="ADD">
         <VBox.margin>
            <Insets left="240.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
